    Id "(" {Exp ","}* ")"  -> PHExp {cons("ThisCall")}
    Exp "." Id "(" {Exp ","}* ")"  -> PHExp {cons("Call")}
    Exp "." Id             -> PHExp {cons("FieldAccess"),prefer} 
    Exp "[" Exp "]"        -> PHExp {cons("CollectionIndex"),prefer}
    Exp "+" Exp  -> PHExp {cons("Add")}
    ConstValue -> PHExp
