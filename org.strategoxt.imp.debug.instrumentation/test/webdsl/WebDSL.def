  context-free syntax

    "event" "(" Id "," "[" { EventArg "," }* "]" ")" -> Exp {cons("EventCall")}
    "event" "(" Id "," "[" { EventArg "," }* "]" ")" -> TemplateElement {cons("EventCallElement")}

    Id ":=" Exp -> EventArg {cons("EventArg")}
  
%%  	replace <element_id> << <templatecall>(<argumenten>);
